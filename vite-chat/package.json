  "repository": {
    "type": "git",
    "url": "https://github.com/feathersjs/playground.git",
    "directory": "vite-chat"
  },
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": ">= 16",
    "npm": ">= 7"
